{
  "name": "@parcnet/client-helpers",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./connection/iframe": {
      "types": "./src/connection/iframe.ts",
      "import": "./dist/connection/iframe.mjs",
      "require": "./dist/connection/iframe.cjs"
    },
    "./connection/websocket": {
      "types": "./src/connection/websocket.ts",
      "import": "./dist/connection/websocket.mjs",
      "require": "./dist/connection/websocket.cjs"
    }
  },
  "scripts": {
    "build": "tsup 'src/**/*@(ts|tsx)' --format cjs,esm --sourcemap",
    "prepublish": "tsup 'src/**/*@(ts|tsx)' --format cjs,esm --dts --clean",
    "lint": "eslint . --max-warnings 0"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "dependencies": {
    "@parcnet/client-rpc": "workspace:*",
    "@pcd/gpc-pcd": "0.0.5",
    "@pcd/pcd-types": "0.11.4",
    "@pcd/pod": "0.1.5",
    "@pcd/podspec": "0.0.2",
    "@pcd/semaphore-identity-pcd": "0.11.4",
    "json-bigint": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@parcnet/eslint-config": "workspace:*",
    "@parcnet/typescript-config": "workspace:*",
    "@types/eslint": "^9.0.0",
    "@types/json-bigint": "^1.0.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/ws": "^8.5.12",
    "eslint": "^9.0.0",
    "tsup": "^8.2.4",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "./connection/iframe": {
        "types": "./dist/connection/iframe.d.ts",
        "import": "./dist/connection/iframe.mjs",
        "require": "./dist/connection/iframe.cjs"
      },
      "./connection/websocket": {
        "types": "./dist/connection/websocket.d.ts",
        "import": "./dist/connection/websocket.mjs",
        "require": "./dist/connection/websocket.cjs"
      }
    }
  }
}
